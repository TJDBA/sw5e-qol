{
  "_examples": {
    "arrayAtEnd": {
      "basePath": "{Item}.system.damage",
      "isArray": false,
      "subpaths": {
        "parts": ".parts[]",
        "firstPart": ".parts[0]",
        "partTypes": ".parts[].type"
      }
    },
    "arrayInMiddle": {
      "basePath": "{Actor}.system.classes",
      "isArray": true,
      "subpaths": {
        "levels": "levels",
        "firstClassLevels": "[0].levels",
        "secondClassLevels": "[1].levels"
      }
    },
    "nestedArrays": {
      "basePath": "{Item}.system.effects",
      "isArray": true,
      "subpaths": {
        "targets": "[].targets[]",
        "firstEffectFirstTarget": "[0].targets[0]",
        "targetNames": "[].targets[].name"
      }
    }
  },
  "actor": {
    "character": {
      "basePath": "{Actor}",
      "isArray": false,
      "subpaths": {
        "prof": "system.attributes.prof",
        "level": "system.details.level",
        "abilities": "system.abilities"
      }
    },
    "class": {
      "basePath": "{Actor}.itemTypes.class",
      "isArray": true,
      "subpaths": {
        "levels": "[].system.levels",
        "name": "[].name",
        "featureAdvancements": "[].system.advancement"
      }
    }
  },
  "power": {
    "ForcePowers": {
      "basePath": "{Actor}.itemTypes.power",
      "isArray": true,
      "filters": {
        "school": ["Universal", "Light", "Dark"]
      },
      "subpaths": {
        "name": ".name",
        "level": ".labels.level",
        "actionType": ".system.actionType",
        "prepared": ".system.prepared",
        "uses": ".system.uses",
        "school": ".labels.school"
      }
    },
    "TechPowers": {
      "basePath": "{Actor}.itemTypes.power",
      "isArray": true,
      "filters": {
        "school": ["Tech"]
      },
      "subpaths": {
        "name": ".name",
        "level": ".labels.level",
        "actionType": ".system.actionType",
        "prepared": ".system.prepared",
        "uses": ".system.uses",
        "school": ".labels.school"
      }
    }
  },
  "ActorItem": {
    "weapon": {
      "basePath": "{Actor}.itemTypes.weapon",
      "isArray": true,
      "subpaths": {
        "id": ".id",
        "name": ".name",
        "attackBonus": ".system.attackBonus",
        "damage": ".system.damage",
        "damageParts": ".system.damage.parts[]",
        "versatileDamage": ".system.damage.versatile",
        "criticalThreshold": ".system.critical.threshold",
        "properties": ".system.properties",
        "save": ".system.save",
        "ability": ".system.ability",
        "ammo": ".system.ammo"
      }
    }
  }
}
